<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fill This In</h:title>
    <model>
      <instance>
        <faims id="Fill_This_In">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <User>
            <User>
              <Select_User/>
              <Login/>
            </User>
          </User>
          <Control>
            <Main>
              <New_Transect/>
              <RecentEntities/>
            </Main>
            <Journal>
              <New_Journal_Entry/>
              <RecentEntities/>
            </Journal>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_Types/>
              <Entity_List/>
            </Search>
            <Identifiers>
              <Next_Unit_ID/>
              <Next_Journal_Entry_Number/>
            </Identifiers>
          </Control>
          <Transect>
            <General_Information>
              <Colgroup_0>
                <Col_0>
                  <StartTimestamp/>
                </Col_0>
                <Col_1>
                  <StartTimestampTrigger/>
                </Col_1>
              </Colgroup_0>
              <Planet_Name/>
              <Planet_Nickname/>
              <author/>
              <Geotag/>
              <Number_Of_Orbits/>
              <Survey_Method/>
              <Predominant_Surface_Type/>
              <Number_of_Features/>
              <Location_and_description/>
            </General_Information>
            <Survey_Procedure>
              <Area_Surveyed/>
              <Bearing/>
              <Colgroup_0>
                <Col_0>
                  <From_Direction/>
                </Col_0>
                <Col_1>
                  <To_Direction/>
                </Col_1>
              </Colgroup_0>
              <Comment_on_transect/>
              <Video_Capture_URL/>
              <Screengrab_Filename/>
              <Attach_a_Screengrab/>
              <Button_Attach_a_Screengrab/>
            </Survey_Procedure>
            <Orbits>
              <New_Orbit/>
              <Orbit_List/>
            </Orbits>
            <Planetary_Coverage>
              <Dominant_Planet_Coverage/>
              <Percentage_of_Visable_Surface/>
              <Comment_on_planetary_coverage/>
            </Planetary_Coverage>
            <Surface_Conditions>
              <Comment_on_surface_conditions/>
              <Features/>
            </Surface_Conditions>
            <Revisit>
              <Revisit/>
              <Revisit_Reason/>
              <Colgroup_0>
                <Col_0>
                  <EndTimestamp/>
                </Col_0>
                <Col_1>
                  <EndTimestampTrigger/>
                </Col_1>
              </Colgroup_0>
            </Revisit>
            <Unit>
              <New_Unit/>
              <Unit_List/>
            </Unit>
          </Transect>
          <Orbit>
            <Orbit>
              <Orbit_Number/>
              <Parent_Planet_Name/>
              <Agriculture/>
              <Architecture/>
              <Communications/>
              <Cultural/>
              <Industry/>
              <Military/>
              <Mining/>
              <Transport/>
              <Settlement/>
              <Other/>
            </Orbit>
          </Orbit>
          <Unit>
            <General_Information>
              <Unit_ID/>
              <Colgroup_0>
                <Col_0>
                  <StartTimestamp/>
                </Col_0>
                <Col_1>
                  <StartTimestampTrigger/>
                </Col_1>
              </Colgroup_0>
              <Parent_Planet_Name/>
              <author/>
              <Geotag_Unit/>
              <Survey_Procedure_Unit/>
              <Survey_State_Unit/>
              <Number_of_Items_Unit/>
              <Location_and_description_Unit/>
            </General_Information>
            <Survey_Procedure>
              <Area_Surveyed_Unit/>
              <Bearing_Unit/>
              <Colgroup_0>
                <Col_0>
                  <From_Direction_Unit/>
                </Col_0>
                <Col_1>
                  <To_Direction_Unit/>
                </Col_1>
              </Colgroup_0>
              <Comment_on_unit/>
              <Video_Capture_URL/>
              <Screengrab_Filename/>
              <Attach_a_Screengrab/>
              <Button_Attach_a_Screengrab/>
            </Survey_Procedure>
            <Survey_Record>
              <New_Survey_Row/>
              <Row_List/>
            </Survey_Record>
            <Vegetation_and_Land_Use>
              <Dominant_Vegetation_Land_Use/>
              <Dominant_Vegetation_Height/>
              <Surface_Visibility/>
              <Comment_on_vegetation_and_land_use/>
            </Vegetation_and_Land_Use>
            <Field_Conditions>
              <Soil_Condition/>
              <Background_Disturbance/>
              <Sherd_Crusting/>
              <Dominant_Surface_Clast_Size/>
              <Clast_Shape/>
              <Soil_Tint/>
              <Soil_Hue/>
              <Comment_on_field_conditions/>
              <Features_Unit/>
            </Field_Conditions>
            <Revisit_Unit>
              <Revisit_Unit/>
              <Revisit_Reason_Unit/>
              <Colgroup_0>
                <Col_0>
                  <EndTimestamp/>
                </Col_0>
                <Col_1>
                  <EndTimestampTrigger/>
                </Col_1>
              </Colgroup_0>
            </Revisit_Unit>
          </Unit>
          <Row>
            <Row>
              <Row_Number/>
              <Parent_Unit_ID/>
              <Parent_Planet_Name/>
              <Metal/>
              <Sherd/>
              <Lithic/>
              <Technology/>
              <Structure/>
              <Wreck/>
              <Monolith/>
              <Settlement/>
              <Other/>
            </Row>
          </Row>
          <Journal>
            <Journal>
              <Colgroup_0>
                <Col_0>
                  <StartTimestamp/>
                </Col_0>
                <Col_1>
                  <StartTimestampTrigger/>
                </Col_1>
              </Colgroup_0>
              <Journal_Entry_Number/>
              <Journal/>
            </Journal>
          </Journal>
          <logic/>
        </faims>
      </instance>
      <bind type="decimal" nodeset="/faims/Control/Identifiers/Next_Unit_ID"/>
      <bind type="decimal" nodeset="/faims/Control/Identifiers/Next_Journal_Entry_Number"/>
      <bind type="decimal" nodeset="/faims/Transect/General_Information/Number_Of_Orbits"/>
      <bind type="decimal" nodeset="/faims/Transect/General_Information/Number_of_Features"/>
      <bind type="decimal" nodeset="/faims/Transect/Survey_Procedure/Area_Surveyed"/>
      <bind type="decimal" nodeset="/faims/Transect/Survey_Procedure/Bearing"/>
      <bind type="decimal" nodeset="/faims/Transect/Planetary_Coverage/Percentage_of_Visable_Surface"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Orbit_Number"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Agriculture"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Architecture"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Communications"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Cultural"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Industry"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Military"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Mining"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Transport"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Settlement"/>
      <bind type="decimal" nodeset="/faims/Orbit/Orbit/Other"/>
      <bind type="decimal" nodeset="/faims/Unit/General_Information/Unit_ID"/>
      <bind type="decimal" nodeset="/faims/Unit/General_Information/Number_of_Items_Unit"/>
      <bind type="decimal" nodeset="/faims/Unit/Survey_Procedure/Area_Surveyed_Unit"/>
      <bind type="decimal" nodeset="/faims/Unit/Survey_Procedure/Bearing_Unit"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Row_Number"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Metal"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Sherd"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Lithic"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Technology"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Structure"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Wreck"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Monolith"/>
      <bind type="decimal" nodeset="/faims/Row/Row/Settlement"/>
      <bind type="decimal" nodeset="/faims/Journal/Journal/Journal_Entry_Number"/>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <group ref="User">
      <label>{User}</label>
      <group ref="User">
        <label>{User}</label>
        <select1 ref="Select_User">
          <label>{Select_User}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <trigger ref="Login">
          <label>{Login}</label>
        </trigger>
      </group>
    </group>
    <group ref="Control">
      <label>{Control}</label>
      <group ref="Main" faims_scrollable="false">
        <label>{Main}</label>
        <trigger ref="New_Transect">
          <label>{New_Transect}</label>
        </trigger>
        <select1 appearance="compact" ref="RecentEntities">
          <label>{Recent_Entities}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
      <group ref="Journal">
        <label>{Journal}</label>
        <trigger ref="New_Journal_Entry">
          <label>{New_Journal_Entry}</label>
        </trigger>
        <select1 appearance="compact" ref="RecentEntities">
          <label>{Recent_Entities}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Records}</label>
        <group faims_style="orientation" ref="Colgroup_0">
          <label/>
          <group faims_style="even" ref="Col_0">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group faims_style="large" ref="Col_1">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search}</label>
            </trigger>
          </group>
        </group>
        <select1 ref="Entity_Types">
          <label>{Entity_Types}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>Options not loaded</value>
          </item>
        </select1>
      </group>
      <group ref="Identifiers">
        <label>{Identifiers}</label>
        <input ref="Next_Unit_ID" faims_style_class="required">
          <label>{Next_Unit_ID}</label>
        </input>
        <input ref="Next_Journal_Entry_Number" faims_style_class="required">
          <label>{Next_Journal_Entry_Number}</label>
        </input>
      </group>
    </group>
    <group ref="Transect" faims_archent_type="Transect">
      <label>{Transect}</label>
      <group ref="General_Information">
        <label>{General_Information}</label>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input faims_attribute_type="measure" ref="StartTimestamp" faims_attribute_name="StartTimestamp">
              <label>{Start_Timestamp}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="even">
            <label/>
            <trigger ref="StartTimestampTrigger">
              <label>{Record_Start_Timestamp}</label>
            </trigger>
          </group>
        </group>
        <input faims_attribute_type="measure" ref="Planet_Name" faims_attribute_name="Planet Name" faims_style_class="required">
          <label>{Planet_Name}</label>
        </input>
        <input faims_attribute_type="measure" ref="Planet_Nickname" faims_attribute_name="Planet Nickname" faims_style_class="required">
          <label>{Planet_Nickname}</label>
        </input>
        <input ref="author" faims_attribute_name="Transect author" faims_attribute_type="measure" faims_read_only="true" faims_annotation="false" faims_certainty="false">
          <label>{Surveyor}</label>
        </input>
        <input faims_attribute_type="measure" ref="Geotag" faims_attribute_name="Geotag" faims_style_class="required">
          <label>{Geotag}</label>
        </input>
        <input faims_attribute_type="measure" ref="Number_Of_Orbits" faims_attribute_name="Number Of Orbits" faims_style_class="required">
          <label>{Number_Of_Orbits}</label>
        </input>
        <select1 faims_attribute_type="vocab" ref="Survey_Method" faims_attribute_name="Survey Method" faims_style_class="required">
          <label>{Survey_Method}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Predominant_Surface_Type" faims_attribute_name="Predominant Surface Type" faims_style_class="required">
          <label>{Predominant_Surface_Type}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Number_of_Features" faims_attribute_name="Number of Features" faims_style_class="required">
          <label>{Number_of_Features}</label>
        </input>
        <input faims_attribute_type="measure" ref="Location_and_description" faims_attribute_name="Location and description">
          <label>{Location_and_description}</label>
        </input>
      </group>
      <group ref="Survey_Procedure">
        <label>{Survey_Procedure}</label>
        <input faims_attribute_type="measure" ref="Area_Surveyed" faims_attribute_name="Area Surveyed" faims_style_class="required">
          <label>{Area_Surveyed__sq__km__}</label>
        </input>
        <input faims_attribute_type="measure" ref="Bearing" faims_attribute_name="Bearing" faims_style_class="required">
          <label>{Bearing__degrees_}</label>
        </input>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <select1 faims_attribute_type="vocab" ref="From_Direction" faims_attribute_name="From Direction" faims_style_class="required">
              <label>{From_Direction}</label>
              <item>
                <label>Options not loaded</label>
                <value>0</value>
              </item>
            </select1>
          </group>
          <group ref="Col_1" faims_style="even">
            <label/>
            <select1 faims_attribute_type="vocab" ref="To_Direction" faims_attribute_name="To Direction" faims_style_class="required">
              <label>{To_Direction}</label>
              <item>
                <label>Options not loaded</label>
                <value>0</value>
              </item>
            </select1>
          </group>
        </group>
        <input faims_attribute_type="measure" ref="Comment_on_transect" faims_attribute_name="Comment on transect">
          <label>{Comment_on_transect}</label>
        </input>
        <input faims_attribute_type="measure" ref="Video_Capture_URL" faims_attribute_name="Video Capture URL" faims_style_class="required">
          <label>{Video_Capture_URL}</label>
        </input>
        <input faims_attribute_type="measure" ref="Screengrab_Filename" faims_attribute_name="Screengrab Filename" faims_style_class="required">
          <label>{Screengrab_Filename}</label>
        </input>
        <select type="file" faims_attribute_type="measure" faims_sync="true" ref="Attach_a_Screengrab" faims_attribute_name="Attach a Screengrab">
          <label>{Attach_a_Screengrab}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <trigger ref="Button_Attach_a_Screengrab">
          <label>{Button_Attach_a_Screengrab}</label>
        </trigger>
      </group>
      <group ref="Orbits" faims_scrollable="false">
        <label>{Orbits}</label>
        <trigger ref="New_Orbit">
          <label>{New_Orbit}</label>
        </trigger>
        <select1 appearance="compact" ref="Orbit_List" faims_annotation="false" faims_certainty="false">
          <label>{Orbit_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
      <group ref="Planetary_Coverage">
        <label>{Planetary_Coverage}</label>
        <select faims_attribute_type="vocab" ref="Dominant_Planet_Coverage" faims_attribute_name="Dominant Planet Coverage">
          <label>{Dominant_Planet_Coverage}</label>
          <!--WARNING: Unexpected attribute "F" ignored-->
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select>
        <input faims_attribute_type="measure" ref="Percentage_of_Visable_Surface" faims_attribute_name="Percentage of Visable Surface" faims_style_class="required">
          <label>{Percentage_of_Visable_Surface}</label>
        </input>
        <input faims_attribute_type="measure" ref="Comment_on_planetary_coverage" faims_attribute_name="Comment on planetary coverage">
          <label>{Comment_on_planetary_coverage}</label>
        </input>
      </group>
      <group ref="Surface_Conditions">
        <label>{Surface_Conditions}</label>
        <input faims_attribute_type="measure" ref="Comment_on_surface_conditions" faims_attribute_name="Comment on surface conditions">
          <label>{Comment_on_surface_conditions}</label>
        </input>
        <input faims_attribute_type="measure" ref="Features" faims_attribute_name="Features">
          <label>{Features}</label>
        </input>
      </group>
      <group ref="Revisit">
        <label>{Revisit}</label>
        <select1 appearance="full" faims_attribute_type="vocab" ref="Revisit" faims_attribute_name="Revisit" faims_style_class="required">
          <label>{Revisit_}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <input faims_attribute_type="measure" ref="Revisit_Reason" faims_attribute_name="Revisit Reason">
          <label>{If_yes__explain_briefly_}</label>
        </input>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input faims_attribute_type="measure" ref="EndTimestamp" faims_attribute_name="EndTimestamp">
              <label>{End_Timestamp}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="even">
            <label/>
            <trigger ref="EndTimestampTrigger">
              <label>{Record_End_Timestamp}</label>
            </trigger>
          </group>
        </group>
      </group>
      <group ref="Unit">
        <label>{Unit}</label>
        <trigger ref="New_Unit">
          <label>{New_Unit}</label>
        </trigger>
        <select1 appearance="compact" ref="Unit_List" faims_annotation="false" faims_certainty="false">
          <label>{Unit_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Orbit" faims_archent_type="Orbit">
      <label>{Orbit}</label>
      <group ref="Orbit">
        <label>{Orbit}</label>
        <input faims_attribute_type="measure" ref="Orbit_Number" faims_attribute_name="Orbit Number" faims_style_class="required">
          <label>{Orbit_Number}</label>
        </input>
        <input faims_attribute_type="measure" ref="Parent_Planet_Name" faims_attribute_name="Parent Planet Name">
          <label>{Parent_Planet_Name}</label>
        </input>
        <input faims_attribute_type="measure" ref="Agriculture" faims_attribute_name="Agriculture">
          <label>{Agriculture}</label>
        </input>
        <input faims_attribute_type="measure" ref="Architecture" faims_attribute_name="Architecture">
          <label>{Architecture}</label>
        </input>
        <input faims_attribute_type="measure" ref="Communications" faims_attribute_name="Communications">
          <label>{Communications}</label>
        </input>
        <input faims_attribute_type="measure" ref="Cultural" faims_attribute_name="Cultural">
          <label>{Cultural}</label>
        </input>
        <input faims_attribute_type="measure" ref="Industry" faims_attribute_name="Industry">
          <label>{Industry}</label>
        </input>
        <input faims_attribute_type="measure" ref="Military" faims_attribute_name="Military">
          <label>{Military}</label>
        </input>
        <input faims_attribute_type="measure" ref="Mining" faims_attribute_name="Mining">
          <label>{Mining}</label>
        </input>
        <input faims_attribute_type="measure" ref="Transport" faims_attribute_name="Transport">
          <label>{Transport}</label>
        </input>
        <input faims_attribute_type="measure" ref="Settlement" faims_attribute_name="Settlement">
          <label>{Settlement}</label>
        </input>
        <input faims_attribute_type="measure" ref="Other" faims_attribute_name="Other">
          <label>{Other}</label>
        </input>
      </group>
    </group>
    <group ref="Unit" faims_archent_type="Unit">
      <label>{Unit}</label>
      <group ref="General_Information">
        <label>{General_Information}</label>
        <input faims_attribute_type="measure" ref="Unit_ID" faims_attribute_name="Unit ID" faims_style_class="required">
          <label>{Unit_ID}</label>
        </input>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input faims_attribute_type="measure" ref="StartTimestamp" faims_attribute_name="StartTimestamp">
              <label>{Start_Timestamp}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="even">
            <label/>
            <trigger ref="StartTimestampTrigger">
              <label>{Record_Start_Timestamp}</label>
            </trigger>
          </group>
        </group>
        <input faims_attribute_type="measure" ref="Parent_Planet_Name" faims_attribute_name="Parent Planet Name" faims_read_only="true">
          <label>{Parent_Planet_Name}</label>
        </input>
        <input ref="author" faims_attribute_name="Unit author" faims_attribute_type="measure" faims_read_only="true" faims_annotation="false" faims_certainty="false">
          <label>{Surveyor}</label>
        </input>
        <input faims_attribute_type="measure" ref="Geotag_Unit" faims_attribute_name="Geotag Unit" faims_style_class="required">
          <label>{Geotag}</label>
        </input>
        <select1 faims_attribute_type="vocab" ref="Survey_Procedure_Unit" faims_attribute_name="Survey Procedure Unit" faims_style_class="required">
          <label>{Survey_Procedure_Unit}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
        <select1 faims_attribute_type="vocab" ref="Survey_State_Unit" faims_attribute_name="Survey State Unit" faims_style_class="required">
          <label>{Is_the_area___}</label>
          <item>
